{
  "name": "redux-create-actions",
  "version": "0.0.1",
  "description": "Redux action creator building utilities",
  "main": "dist/redux-create-actions.cjs.js",
  "module": "dist/redux-create-actions.esm.js",
  "browser": "dist/redux-create-actions.umd.js",
  "scripts": {
    "test": "NODE_ENV=test nyc tape ./tests/*.spec.js",
    "build": "rollup -c",
    "lint": "eslint src/*.js",
    "flow": "flow",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "prebuild": "rm -rf dist"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": "https://github.com/adrianObel/redux-create-actions",
  "author": "Adrian Obelmejias <adrian@obel.me>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "nyc": "^11.1.0",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-babel-minify": "^3.1.2",
    "tape": "^4.8.0"
  }
}
